<div class="page-title">	<i class="icon-custom-left"></i>

  <h3>Subscription - <span class="semi-bold">Logs</span></h3>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="grid simple ">
      <div class="grid-title no-border">

      </div>
      <div class="grid-body no-border">
        <h3><span class="semi-bold">Logs</span></h3>
        <table class="table no-more-tables">
          <thead>
          <tr>
            <th style="width:14%">Subscription Id</th>
            <th style="width:27%">Message</th>
            <th style="width:6%">Request time</th>
          </tr>
          </thead>
          <tbody>

          <% @logs[:data][:log_entries].each do |log_item| %>
            <tr>
              <td class="v-align-middle"><%= log_item[:subscription_id] %></td>
              <td class="v-align-middle"><span class="muted"><%= log_item[:message] %></span>
              </td>
              <td><span class="muted"><%= log_item[:request_time] %></span>
              </td>
            </tr>
          <% end %>

          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
